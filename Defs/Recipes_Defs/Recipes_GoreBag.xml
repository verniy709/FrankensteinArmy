<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>Frankenstein_CollectGoreBag_NewMeat</defName>
    <label>(Frankenstein)CollectGoreBag_NewMeat</label>
    <description>ToDo.</description>
    <jobString>Dissecting.</jobString>
    <workAmount>200</workAmount>
    <workSpeedStat>MechFormingSpeed</workSpeedStat>
    <effectWorking>ButcherFlesh</effectWorking>
    <soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
    <uiIconThing>Meat_Muffalo</uiIconThing>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Corpses</li>
          </categories>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Corpses</li>
      </categories>
      <disallowedCategories>
        <li>CorpsesAnimal</li>
        <li>CorpsesInsect</li>
        <li>CorpsesMechanoid</li>
        <!-- <li>FrankensteinArmyCorpse</li> -->
      </disallowedCategories>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
      <categories>
        <li>CorpsesHumanlike</li>
      </categories>
    </defaultIngredientFilter>
    <products>
      <Frankenstein_GoreBag>120</Frankenstein_GoreBag>
    </products>
  </RecipeDef>


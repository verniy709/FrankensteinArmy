<Defs>

  <!-- Frankenstein_Soldier_SubMachinegun-->
  <PawnKindDef ParentName="LightMechanoidKind">
    <defName>Frankenstein_Soldier_SubMachinegun</defName>
    <label>(FA)soldier submachine gun</label>
    <labelPlural>(FA)Soldiers submachine gun</labelPlural>
    <race>Frankenstein_Soldier_SubMachinegun</race>
    <combatPower>100</combatPower>
    <allowInMechClusters>false</allowInMechClusters>
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>Mechanoid/Frankenstein_Soldier_SubMachinegun</texPath>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>1.9</drawSize>
          <shadowData>
            <volume>(0.3, 0.5, 0.3)</volume>
          </shadowData>
        </bodyGraphicData>
      </li>
    </lifeStages>    
    <weaponTags>
      <li>Frankenstein_SubMachinegun</li>
    </weaponTags>
  </PawnKindDef>


<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Frankenstein_SdKfz251_Flak-->
  <PawnKindDef ParentName="HeavyMechanoidKind">
    <defName>Frankenstein_SdKfz251_Flak</defName>
    <label>(Frankenstein)SdKfz251_Flak</label>
    <labelPlural>(Frankenstein)SdKfz251_Flaks</labelPlural>
    <race>Frankenstein_SdKfz251_Flak</race>
    <combatPower>900</combatPower>
    <maxPerGroup>2</maxPerGroup>
    <allowInMechClusters>false</allowInMechClusters>  
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>Mechanoid/Frankenstein_SdKfz251_Flak</texPath>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>4.0</drawSize>
          <shadowData>
            <volume>(0.7, 0.8, 0.7)</volume>
          </shadowData>
        </bodyGraphicData>
      </li>
    </lifeStages>    
    <controlGroupPortraitZoom>0.7</controlGroupPortraitZoom>  
    <weaponTags>
      <li>Frankenstein_Flak</li>
    </weaponTags>
  </PawnKindDef>


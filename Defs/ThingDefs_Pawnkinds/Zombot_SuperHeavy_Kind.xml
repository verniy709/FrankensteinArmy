<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Frankenstein_PanzerIV-->
  <PawnKindDef ParentName="HeavyMechanoidKind">
    <defName>Frankenstein_PanzerIV</defName>
    <label>(FA)Panzer IV</label>
    <labelPlural>(FA)Panzer IVs</labelPlural>
    <race>Frankenstein_PanzerIV</race>
    <combatPower>800</combatPower>
    <maxPerGroup>2</maxPerGroup>
    <allowInMechClusters>false</allowInMechClusters>  
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>Mechanoid/Frankenstein_PanzerIV</texPath>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>4.2</drawSize>
          <shadowData>
            <volume>(0.7, 0.8, 0.7)</volume>
          </shadowData>
        </bodyGraphicData>
      </li>
    </lifeStages>    
    <controlGroupPortraitZoom>0.7</controlGroupPortraitZoom>  
    <weaponTags>
      <li>Frankenstein_Flak</li>
    </weaponTags>
  </PawnKindDef>

